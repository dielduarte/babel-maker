{
  "name": "babel-maker",
  "version": "0.0.4",
  "description": "A cli to create initial babel configuration in a easy way",
  "main": "lib/index.js",
  "scripts": {
    "test": "tape -r babel-register tests/**/*.js",
    "start": "babel-node src/index.js",
    "build": "babel src -d lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dielduarte/babel-maker.git"
  },
  "keywords": [
    "babel",
    "babel-maker",
    "automatic-babel"
  ],
  "files": [
    "lib/"
  ],
  "bin": {
    "babel-maker": "lib/index.js"
  },
  "author": "Diel Duarte",
  "license": "MIT",
  "dependencies": {
    "inquirer": "^5.0.0",
    "install": "^0.10.2",
    "npm": "^5.6.0",
    "ora": "^1.3.0",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.38",
    "babel-cli": "^6.26.0",
    "babel-plugin-jsx-event-modifiers": "^2.0.5",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue": "^2.0.0",
    "babel-register": "^6.26.0",
    "tape": "^4.8.0"
  }
}
